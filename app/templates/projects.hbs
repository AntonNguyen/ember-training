<h2>Projects</h2>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Manager</th>
    <th>Client</th>
  </tr>
    {{#each}}
        <tr>
          <td>{{#link-to 'project' this}}{{name}}{{/link-to}}</td>
          <td>{{description}}</td>
          <td>{{managerName}}</td>
          <td>{{clientName}}</td>
        </tr>
    {{/each}}
</table>
